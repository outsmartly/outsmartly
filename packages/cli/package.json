{
  "name": "@outsmartly/cli",
  "description": "Command Line Interface for Outsmartly.",
  "version": "0.0.77",
  "license": "MIT",
  "bin": {
    "outsmartly": "bin/outsmartly"
  },
  "main": "lib/index.js",
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/outsmartly/outsmartly.git",
    "directory": "packages/cli"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "outsmartly",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-not-found",
      "@oclif/plugin-warn-if-update-available"
    ]
  },
  "scripts": {
    "build": "echo 'cli: no build required.'",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo 'cli: no tests available.'",
    "version": "oclif-dev readme && git add README.md",
    "outsmartly": "node --unhandled-rejections=strict ./bin/outsmartly.cjs",
    "prepublishOnly": "rm tsconfig.tsbuildinfo || true"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.12.16",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-autocomplete": "^0.2.0",
    "@oclif/plugin-help": "^3",
    "@oclif/plugin-not-found": "^1.2.4",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-typescript": "^6.0.0",
    "@rollup/plugin-virtual": "^2.0.3",
    "abortcontroller-polyfill": "^1.5.0",
    "chalk": "^4.1.0",
    "chokidar": "^3.4.2",
    "cli-spinners": "^2.4.0",
    "comlink": "^4.3.0",
    "fs-extra": "^8.1.0",
    "inquirer": "^7.3.3",
    "multiline-template": "^1.1.0",
    "node-fetch": "3.0.0-beta.9",
    "open": "^8.0.2",
    "openid-client": "^4.4.2",
    "ora": "^5.1.0",
    "rollup": "^2.34.2",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "rollup-plugin-terser": "^7.0.2",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.0.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/fs-extra": "^9.0.12",
    "@types/inquirer": "^7.3.1",
    "@types/node": "^16.4.13",
    "@types/node-fetch": "^2.5.7",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "nyc": "^14",
    "typescript": "^4.0.3"
  },
  "resolutions": {
    "rollup": "2.28.1"
  }
}
